    <div id="codeModal" class="modal-overlay hidden">
        <div class="modal-content">
            <button class="modal-close-btn">&times;</button>
            <h3 id="modalCodeTitle"></h3>
            <pre><code id="modalCodeContent"></code></pre>
        </div>
    </div>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const expandButtons = document.querySelectorAll('.expand-btn');
            const showCodeModalButtons = document.querySelectorAll('.show-code-modal-btn');
            const codeModal = document.getElementById('codeModal');
            const modalCodeTitle = document.getElementById('modalCodeTitle');
            const modalCodeContent = document.getElementById('modalCodeContent');
            const modalCloseBtn = document.querySelector('.modal-close-btn');

            expandButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const projectRow = button.closest('.project-row');
                    const fullDescription = projectRow.querySelector('.full-description');
                    const truncatedDescription = projectRow.querySelector('.truncated-description');
                    const isExpanded = button.getAttribute('aria-expanded') === 'true';

                    if (isExpanded) {
                        fullDescription.classList.add('hidden');
                        truncatedDescription.classList.remove('hidden');
                        button.setAttribute('aria-expanded', 'false');
                        button.querySelector('i').classList.remove('fa-chevron-up');
                        button.querySelector('i').classList.add('fa-chevron-down');
                        projectRow.classList.remove('expanded');
                    } else {
                        fullDescription.classList.remove('hidden');
                        truncatedDescription.classList.add('hidden');
                        button.setAttribute('aria-expanded', 'true');
                        button.querySelector('i').classList.remove('fa-chevron-down');
                        button.querySelector('i').classList.add('fa-chevron-up');
                        projectRow.classList.add('expanded');
                    }
                });
            });

            showCodeModalButtons.forEach(button => {
                button.addEventListener('click', () => {
                    const codeId = button.dataset.codeId;
                    const codeSourceElement = document.getElementById(codeId);
                    let codeTitle = button.textContent.trim();
                    codeTitle = codeTitle.charAt(0).toUpperCase() + codeTitle.slice(1);

                    if (codeSourceElement) {
                        modalCodeTitle.textContent = codeTitle;
                        modalCodeContent.textContent = codeSourceElement.textContent.trim();
                        codeModal.classList.remove('hidden');
                        document.body.style.overflow = 'hidden';
                    }
                });
            });

            function closeCodeModal() {
                codeModal.classList.add('hidden');
                modalCodeTitle.textContent = '';
                modalCodeContent.textContent = '';
                document.body.style.overflow = '';
            }

            modalCloseBtn.addEventListener('click', closeCodeModal);

            codeModal.addEventListener('click', (event) => {
                if (event.target === codeModal) {
                    closeCodeModal();
                }
            });

            document.addEventListener('keydown', (event) => {
                if (event.key === 'Escape' && !codeModal.classList.contains('hidden')) {
                    closeCodeModal();
                }
            });

            document.querySelectorAll('.project-description-wrapper p').forEach(p => {
                if (p.classList.contains('truncated-description')) {
                    const originalTextElement = p.nextElementSibling;
                    if (originalTextElement) {
                        const originalText = originalTextElement.textContent.trim();
                        const maxLength = 250;
                        if (originalText.length > maxLength) {
                            p.textContent = originalText.substring(0, maxLength) + '... ';
                            p.classList.remove('hidden');
                            originalTextElement.classList.add('hidden');
                        } else {
                            p.textContent = originalText;
                            p.classList.remove('hidden');
                            originalTextElement.classList.add('hidden');
                            const expandButton = p.closest('.project-description-wrapper').previousElementSibling ? p.closest('.project-description-wrapper').previousElementSibling.querySelector('.expand-btn') : null;
                            if (expandButton) {
                                expandButton.style.display = 'none';
                            }
                        }
                    }
                }
            });

            document.querySelectorAll('.code-source').forEach(codeBlock => {
                codeBlock.classList.add('hidden');
            });
        });
    </script>
